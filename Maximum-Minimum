#include <stdio.h>
int main()
{
	int n1 , n2 , n3 , max;
	printf("Enter the first number ");
	scanf("%d",&n1);
	printf("Enter the second number ");
	scanf("%d",&n2);
	printf("Enter the third number ");
	scanf("%d",&n3);
	max = (n1 > n2) ? (n1 > n3 ? n1 : n3) : (n2 > n3 ? n2 : n3);
	printf("Largest number among %d, %d and %d is %d.",	n1, n2, n3, max);
	return 0;
}
